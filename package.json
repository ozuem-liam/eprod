{
  "name": "eprod",
  "version": "1.0.0",
  "description": "mainstack phase 1 test",
  "main": "index.js",
  "scripts": {
    "dev": "yarn run nodemon",
    "start:start": "yarn run build && node build/index.js",
    "start": "set APP_ENV=production; node -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config build/index.js",
    "build": "tsc -p ./tsconfig.json",
    "start:prod": "yarn run build && yarn run start",
    "nodemon": "nodemon --watch \"src/**/*.ts\" --ignore \"src/**/*.spec.ts\" -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config --exec \"ts-node --files\" src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check ."
  },
  "author": "williams",
  "license": "MIT",
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.13.0",
    "cloudinary": "^1.34.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-mongo-sanitize": "^2.2.0",
    "hpp": "^0.2.3",
    "http-status": "^1.6.2",
    "joi": "^17.7.1",
    "mongoose": "^6.9.1",
    "morgan": "^1.10.0",
    "nanoid": "3.1.29",
    "nodemon": "^2.0.20",
    "pino": "^8.10.0",
    "slugify": "^1.6.5",
    "typescript": "^4.9.5",
    "winston": "^3.8.2",
    "xml2js": "^0.4.23",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/hpp": "^0.2.2",
    "@types/morgan": "^1.9.4",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  }
}
